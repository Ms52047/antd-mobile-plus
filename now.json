{
  "name": "ui",
  "scope": "alitajs",
  "version": 2,
  "builds": [{
    "src": "build.sh",
    "use": "@vercel/static-build",
  }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "github": {
    "enable": false
  },
  "alias": ["ui.alitajs.com"]
}
