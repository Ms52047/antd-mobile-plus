{
  "name": "ui",
  "scope": "alitajs",
  "version": 2,
  "builds": [{
    "src": "build.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "dist"
    }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/404.html"
    }
  ],
  "github": {
    "enable": false
  },
  "alias": ["ui.alitajs.com"]
}
